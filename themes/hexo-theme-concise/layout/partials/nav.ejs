<% if(theme.themeColor){ %>
<div class="nav-wrapper">
    <% }else { %>
    <div class="nav-wrapper" id="header">
        <% } %>
        <div class="container">
            <nav>


                <div class="logo wave">
                    <a href="<%- url_for() %>" id="logo">
                        <%- theme.blogname %>
                    </a>
                </div>


                <div class="nav-toggle-icon">
                    <div class="material-hamburger">
                    <span>
                    </span>
                        <span>
                    </span>
                        <span>
                    </span>
                    </div>
                </div>

                <div class="menu-wrapper">
                    <div class="nav-indicator">
                    </div>
                    <ul class="menus">
                        <% for (var i in theme.menu){ %>
                            <li class="">
                                <a class="" href="<%- url_for(theme.menu[i]) %>">
                                    <%- __(i) %>
                                </a>
                            </li>
                        <% } %>
                    </ul>
                </div>

            </nav>
        </div>
    </div>
</div>
